{
    "name": "gouguoyin/laravel-log-viewer",
    "description": "A Laravel log reader",
    "license": "MIT",
    "keywords": ["log", "log-reader", "log-viewer", "logging", "laravel", "lumen"],
    "type": "laravel-package",
    "authors": [
        {
            "name": "gouguoyin",
            "email": "raphaelht@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*|5.*|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-0": {
            "Gouguoyin\\LogViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gouguoyin\\LogViewer\\LogViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
